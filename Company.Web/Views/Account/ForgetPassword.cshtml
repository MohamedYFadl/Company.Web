@model ForgetPasswordModel
@{
    ViewData["Title"] = "ForgetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<h1>Forget Password</h1>
<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="ForgetPassword">
			<input type="email" asp-for="Email" placeholder="Email" class="email text">
			<div asp-validation-for="Email"></div>
			<div asp-validation-summary="All" class="text-danger"></div>
			<input type="submit" value="ResetPassword">
		</form>
	</div>
</div>

